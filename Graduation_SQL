SELECT DISTINCT a.spriden_id,
                b.shrdgmr_degc_code,
                b.shrdgmr_majr_code_1,
                b.shrdgmr_term_code_grad,
                b.shrdgmr_grad_date,
                c.stvcoll_code,
                CASE WHEN c.stvcoll_desc = 'Mathematics' THEN 'Coll of Sci, Engr & Tech'
                       WHEN c.stvcoll_desc = '* Natural Sciences' THEN 'Coll of Sci, Engr & Tech'
                       WHEN c.stvcoll_desc = '*Education/Family Studies/PE' THEN 'College of Education'
                       WHEN c.stvcoll_desc = 'Humanities & Social Sciences' THEN 'Coll of Humanities/Soc Sci'
                       WHEN c.stvcoll_desc = 'History/Political Science' THEN 'Coll of Humanities/Soc Sci'
                       WHEN c.stvcoll_desc = 'Computer Information Tech' THEN 'Coll of Sci, Engr & Tech'
                       WHEN c.stvcoll_desc = 'Technologies' THEN 'Coll of Sci, Engr & Tech' ---Is this the correct place for this??
                       ELSE c.stvcoll_desc
                END AS stvcoll_desc,
                d.stvmajr_desc,
                e.sgbuser_term_code AS Cohort_Group,
                e.sgbuser_sudc_code,
                f.stvsudc_desc
     FROM spriden a
     LEFT JOIN shrdgmr b
     ON b.shrdgmr_pidm = a.spriden_pidm
     LEFT JOIN stvcoll c
     ON b.shrdgmr_coll_code_1 = c.stvcoll_code
     LEFT JOIN sgbuser e
     ON b.shrdgmr_pidm = e.sgbuser_pidm
     LEFT JOIN stvmajr d
     ON b.shrdgmr_majr_code_1 = d.stvmajr_code
     LEFT JOIN stvsudc f
     ON e.sgbuser_sudc_code = f.stvsudc_code
WHERE b.shrdgmr_degs_code = 'AW'
  AND b.shrdgmr_levl_code = 'UG'
  AND b.shrdgmr_term_code_grad = '201920'
  AND a.spriden_change_ind IS NULL;
